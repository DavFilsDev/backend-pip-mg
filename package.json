{
  "name": "backend-pip-mg",
  "version": "1.0.0",
  "description": "Backend API pour la Plateforme d'Insertion Professionnelle Madagascar - Open Source, gratuit, Node.js/Express/TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "prepare": "husky install"
  },
  "keywords": [
    "madagascar",
    "emploi",
    "entretien",
    "cv",
    "open-source",
    "nodejs",
    "express"
  ],
  "author": "F. Miharisoa David Fils RATIANDRAIBE <miharisoadavidfils@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavFilsDev/backend-pip-mg.git"
  },
  "bugs": {
    "url": "https://github.com/DavFilsDev/backend-pip-mg/issues"
  },
  "homepage": "https://github.com/DavFilsDev/backend-pip-mg#readme",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^8.0.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1"
  }
}
